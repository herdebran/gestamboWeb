	<queryString>
		<![CDATA[select cli.id nroCliente, concat(cli.apellido, ', ', cli.nombre ) nombreCliente, cli.condicionMorosidad,
 l.nombre as localidad, case when concat(cli.caracteristica, cli.telefono, cli.caracteristica1, cli.telefono1) is null then ''
else concat(cli.caracteristica, ' ', cli.telefono, ' cel.', cli.caracteristica1, ' ', cli.telefono1) end as telefonos, s.cantidadCuotasSolicitadas, c.montoFinal
 from Credito c
inner join Cuota cuo on c.id = cuo.credito_id
inner join SolicitudAprobacionCredito s on s.id = c.solicitudAprobacionCredito_id
inner join Cliente cli on cli.id = c.cliente_id
inner join Localidad l on l.id = cli.localidad_id
where c.estado = 'ACTIVO' and cuo.estado = 'PAGA' and
(select count(id) from Credito where estado = 'ACTIVO' and cliente_id = c.cliente_id group by cliente_id having count(id) = 1)
and
c.id in (select credito.id id
from Cuota cuota inner join Credito credito on credito.id = cuota.credito_id
where credito.cliente_id = c.cliente_id and credito.estado = 'ACTIVO'
group by credito.id
having max(cuota.fechaVencimiento) <  adddate(now(), interval (11 - dayofmonth(now())) day))
group by c.cliente_id, c.id, s.cantidadCuotasSolicitadas
having count(*) = s.cantidadCuotasSolicitadas -1
and max(cuo.fechaVencimiento) < adddate(now(), interval (11 - dayofmonth(now())) day)
order by c.montoFinal desc, condicionMorosidad]]>
	</queryString>
