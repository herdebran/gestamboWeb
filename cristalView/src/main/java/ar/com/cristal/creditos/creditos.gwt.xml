<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='creditos'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name='de.novanic.eventservice.GWTEventService'/>
  
	<inherits name="com.google.gwt.user.Debug" />
	<inherits name="com.google.gwt.uibinder.UiBinder" />
	<inherits name="ar.com.snoop.gwt.commons.GwtCommons"/>
	<inherits name="com.google.gwt.place.Place"/>
	<inherits name="com.google.gwt.activity.Activity"/>


<!-- For development, a default of `DEBUG` is recommended -->
<!--   <inherits name="com.allen_sauer.gwt.log.gwt-log-DEBUG" /> -->
    <inherits name="com.allen_sauer.gwt.log.gwt-log-OFF" />



  <entry-point class='ar.com.cristal.creditos.client.Creditos'/>
  
  
  <stylesheet src="library/css/CristalView.css"/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>
  
  
    
  
  <!-- Use ClientFactoryImpl by default -->
    <replace-with class="ar.com.cristal.creditos.client.ClientFactoryImpl">
    <when-type-is class="ar.com.cristal.creditos.client.ClientFactory"/>
    </replace-with>

</module>
