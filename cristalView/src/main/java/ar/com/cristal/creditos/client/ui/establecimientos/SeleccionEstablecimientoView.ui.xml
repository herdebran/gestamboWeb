<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:snoop="urn:import:ar.com.snoop.gwt.commons.client.widget">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
			<div ui:field="containerPopUp">
			<div class="gwt-TitledPanel">
			<div class="title">Selecci&oacute;n de Establecimiento</div>
			<div class="clear20"></div>
			<table>
					<thead></thead>
						<tbody>
							<tr>
								<td>Establecimiento</td>
								<td><snoop:ListBox ui:field="cmbEstablecimiento" multipleSelect="false" width="160px" enabled="true"></snoop:ListBox></td>
							</tr>
						</tbody>
			</table>
			
			<div class="clear20"></div>
			<div style="float:right">
 				<g:Button ui:field="btnSeleccionar" styleName="gwt-Button" text="Seleccionar" visible="true"></g:Button>
 				<g:Button ui:field="btnCerrar" styleName="gwt-Button" text="Cerrar" visible="true"></g:Button>
 			</div>
			<div class="clear20"></div>
	</div>
	</div>
	</g:HTMLPanel>
</ui:UiBinder> 